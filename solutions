//solution here
#include<iostream>
using namespace std;
int main()
{
  int num, i;
  bool flag = true;
  cin>>num;
  for(i=2;i<=num/2;i++)
  {
    if(num%i==0)
     {
      flag = false;
      break;
     }
  }
  if(flag=true) cout<<num<<" is prime.\n";
  else cout<<num<<" is not prime.\n";
  return 0;
}
